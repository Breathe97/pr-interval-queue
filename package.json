{
  "name": "pr-interval-queue",
  "version": "1.1.0",
  "description": "一个全局的事件队列，使用一个计时器然后根据时间线队列来帮助你实现一些方法的重复调用。",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "build": "tsc && vite build"
  },
  "dependencies": {
    "pr-tools": "^1.0.9"
  },
  "devDependencies": {
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.3"
  },
  "private": false,
  "author": "Breathe",
  "keywords": [
    "pr-interval-queue",
    "事件队列",
    "计时器"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Breathe97/pr-interval-queue"
  }
}
